<?xml version="1.0" encoding="utf-8"?>

<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   namespace="EWG.Domain.Entities.Dictionaries"
                   assembly="EWG.Domain">

    <class name="GameMap"
           table="DicGameMaps">
        <id name="Id">
            <generator class="native" />
        </id>
        <property name="PublicCode" column="publicCode" />
        <property name="DateStart" column="dateStart" />
        <property name="DateEnd" column="dateEnd" />
        <property name="Name" column="name" />
        <property name="ResourceName" column="resourceName" />
    </class>
</hibernate-mapping>